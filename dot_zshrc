export GEM_HOME="$HOME/.gem"

export CLICOLORS=1
export PATH="$HOME/go/bin:/opt/homebrew/opt/ruby/bin:$GEM_HOME/bin:$PATH"

export LDFLAGS="-L/opt/homebrew/opt/ruby/lib"
export CPPFLAGS="-I/opt/homebrew/opt/ruby/include"
export PKG_CONFIG_PATH="/opt/homebrew/opt/ruby/lib/pkgconfig"


# The next line updates PATH for the Google Cloud SDK.
if [ -f '/Users/andreb/Downloads/google-cloud-sdk/path.zsh.inc' ]; then . '/Users/andreb/Downloads/google-cloud-sdk/path.zsh.inc'; fi

# The next line enables shell command completion for gcloud.
if [ -f '/Users/andreb/Downloads/google-cloud-sdk/completion.zsh.inc' ]; then . '/Users/andreb/Downloads/google-cloud-sdk/completion.zsh.inc'; fi

